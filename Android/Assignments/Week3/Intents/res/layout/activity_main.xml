<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jmc.android_course.intents.MainActivity"
    tools:ignore="MergeRootFrame" >
<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
        <Button
        android:id="@+id/data_intent_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Intent" />
    
    <Button
        android:id="@+id/call_intent_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call Intent" />
    
    <Button
        android:id="@+id/search_intent_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search Intent" />
        
    </LinearLayout>
</FrameLayout>
